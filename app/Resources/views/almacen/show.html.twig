{% extends 'base.html.twig' %}

{% block body %}
    <h1>Almacen</h1>

    <table>
        <tbody>
            <tr>
                <th>Nombrealmacen</th>
                <td>{{ almacen.nombrealmacen }}</td>
            </tr>
            <tr>
                <th>Idalmacen</th>
                <td>{{ almacen.idalmacen }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('almacen_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('almacen_edit', { 'id': almacen.idalmacen }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
