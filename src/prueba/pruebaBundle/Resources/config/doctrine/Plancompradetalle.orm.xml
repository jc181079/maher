<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="prueba\pruebaBundle\Entity\Plancompradetalle" table="plancompradetalle">
    <indexes>
      <index name="fk_plancompradetalle_plancompra1_idx" columns="idplancompra"/>
      <index name="fk_plancompradetalle_producto1_idx" columns="idproducto"/>
    </indexes>
    <id name="idplancompradetalle" type="integer" column="idplancompradetalle">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cantidad" type="integer" column="cantidad" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idproducto" target-entity="Producto" fetch="LAZY">
      <join-columns>
        <join-column name="idproducto" referenced-column-name="idproducto"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idplancompra" target-entity="Plancompra" fetch="LAZY">
      <join-columns>
        <join-column name="idplancompra" referenced-column-name="idplancompra"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
